<%= form_with(model: @user, builder: LesliView::Forms::Builder) do |form| %>
    <%= form.fieldset do %>
        <%= form.field(:alias) %>
        <%= form.field(:title) %>
        <%= form.field(:email) %>
        <%= form.field(:first_name) %>
        <%= form.field(:last_name) %>
        <%= form.field(:telephone) %>
        <hr/>
        <%= form.button %>
    <% end %>        
<% end %>
